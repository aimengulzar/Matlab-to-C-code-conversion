////////////// MATLAB CODE ////////////////
a=[1:3;4:6;7:9]
b=[1:3;4:6;7:9]
sum=a+b

///////////////////// C CODE IS FOLLOWING /////////////////
#include <stdio.h>
#include<conio.h>
void main()
{
	int a[3][3];
	int b[3][3];
	int sum[3][3];
	int i, j;
	
	printf("Enter  matrix a= \n ");
	
	for (i = 0; i < 3; i++)
	{
		for (j = 0; j < 3; j++)
		{


			scanf("%d", &a[i][j]);

		}
		

	}
	printf("\n");
	printf("Enter  matrix b= \n");
	
	for (i = 0; i < 3; i++)
	{
		for (j = 0; j < 3; j++)
		{

			scanf("%d", &b[i][j]);


		}
		
	}
	printf("\n");
	printf("sum  matrix = \n");
	
	for (i = 0; i < 3; i++)
	{
		for (j = 0; j < 3; j++)
		{

			sum[i][j] = a[i][j] + b[i][j];
			printf(" %d ", sum[i][j]);

		}
		printf("\n");
	}




	getch();
}

